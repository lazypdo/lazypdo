{
    "name": "lazypdo/lazypdo",
    "description": "LazyPDO is a set of wrappers over PHP's standard PDO and PDOStatement classes. It enables lazy loading, serialization and decoration.",
    "keywords": [
        "test",
        "unit test",
        "pdo",
        "mock",
        "lazyload",
        "lazy load",
        "serializable",
        "serialize"
    ],
    "homepage": "https://github.com/lazypdo/lazypdo",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LazyPDO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LazyPDO\\Test\\": "test/"
        }
    }
}
